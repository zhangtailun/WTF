					["colorFunc"] = "function(progress, r1, g1, b1, a1, r2, g2, b2, a2)\n    local e = aura_env\n    local s = e.state\n    local r = e.region\n    local p = e.Pallete\n    local bg = r.bar.bg\n    \n    if s then\n        if s.additionalProgress[1].max > 0 then\n            r:SetOverlayColor(1, p.Spend.r, p.Spend.g, p.Spend.b, p.Spend.a)\n        end\n        \n        bg:SetVertexColor(p.Background.r, p.Background.g, p.Background.b, p.Background.a)\n        \n        if s.progressType == \"timed\" then\n            r:SetInverse(true)\n            \n            return p.Doom.r, p.Doom.g, p.Doom.b, p.Doom.a\n        else\n            r:SetInverse(false)\n            \n            if s.full then\n                return p.Full.r, p.Full.g, p.Full.b, p.Full.a\n            elseif e.ColorShardsSeparately then\n                local fg = \"Shard\"..s.index\n                \n                return p[fg].r, p[fg].g, p[fg].b, p[fg].a\n            else\n                return p.Foreground.r, p.Foreground.g, p.Foreground.b, p.Foreground.a\n            end\n        end\n    end\nend",
